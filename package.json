{
  "name": "webxr-start",
  "version": "0.0.1",
  "description": "WebXR Start!",
  "main": "src/main.js",
  "author": "dancingfrog",
  "license": "CC0-1.0",
  "dependencies": {
    "@iwer/devui": "^0.2.1",
    "esbuild": "^0.24.2",
    "gamepad-wrapper": "^1.3.4",
    "gsap": "^3.12.5",
    "iwer": "^1.0.4",
    "three": "^0.170.0"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@vitejs/plugin-basic-ssl": "^1.2.0",
    "rollup-plugin-shader": "^0.0.3",
    "vite": "^5.4.11"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite --host 0.0.0.0",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview --host 0.0.0.0",
    "render:readme": "quarto render README.qmd --to gfm --output README.md",
    "start": "vite --host 0.0.0.0 --open",
    "start-local": "vite --config ../vite.config.local.js",
    "test": "jest --no-cache",
    "test:clean": "jest --clearCache",
    "type:check": "tsc --noEmit"
  }
